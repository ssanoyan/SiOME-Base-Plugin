{
  "name": "siome-base-plugin",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "prettier": "prettier --write src/**/*.{js,html,css,scss,ts,json}",
    "prestart": "npm run export-config-for-browser",
    "start": "ng serve",
    "prebuild": "npm run export-config-for-browser",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "clean-releases": "rimraf -- ./dist/releases/* ",
    "prebuild-plugin": "npm run export-config-for-webcomponent",
    "build-plugin": "ng build && node build-plugins.js",
    "export-config-for-browser": "node -p \"'export const name = ' + JSON.stringify(require('./package.json').name) + '; \\r\\nexport const version = ' + JSON.stringify(require('./package.json').version) + '; \\r\\nexport const as_web_component = false;' \" > src/config.ts",
    "export-config-for-webcomponent": "node -p \"'export const name = ' + JSON.stringify(require('./package.json').name) + '; \\r\\nexport const version = ' + JSON.stringify(require('./package.json').version) + '; \\r\\nexport const as_web_component = true;' \" > src/config.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.4",
    "@angular/cdk": "^17.0.1",
    "@angular/common": "^17.0.4",
    "@angular/compiler": "^17.0.4",
    "@angular/core": "^17.0.4",
    "@angular/elements": "^17.0.4",
    "@angular/forms": "^17.0.4",
    "@angular/material": "^17.0.1",
    "@angular/platform-browser": "^17.0.4",
    "@angular/platform-browser-dynamic": "^17.0.4",
    "@angular/router": "^17.0.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "typedoc-plugin-markdown": "^4.4.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.3",
    "@angular-eslint/builder": "17.0.1",
    "@angular-eslint/eslint-plugin": "17.0.1",
    "@angular-eslint/eslint-plugin-template": "17.0.1",
    "@angular-eslint/schematics": "17.0.1",
    "@angular-eslint/template-parser": "17.0.1",
    "@angular/cli": "~17.0.3",
    "@angular/compiler-cli": "^17.0.4",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "concat": "^1.0.3",
    "document-register-element": "^1.14.10",
    "eslint": "^8.47.0",
    "fs-extra": "^11.1.1",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.0.3",
    "typescript": "~5.2.2"
  }
}
